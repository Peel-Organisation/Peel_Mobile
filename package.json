{
  "name": "Peel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "clean": "cd android && ./gradlew clean && cd ..",
    "deleteCache": "shx rm -r ./node_modules/.cache && npm cache clean --force && npm install",
    "deleteBuild": "shx rm -r android/app/build && shx rm -r android/build && shx rm -r android/.gradle && shx rm -r android/.idea && shx rm -r package-lock.json && shx rm -r nodes_modules && npm install",
    "test": "jest",
    "lint": "eslint .",
    "install-ios": "npm i && bundle install && cd ./ios && bundle exec pod install",
    "install-android": "npm i"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "^2.1.0",
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/geolocation": "^3.0.4",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/messaging": "^17.4.2",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.6",
    "axios": "^1.2.1",
    "dateformat": "^5.0.3",
    "i18next": "^22.4.8",
    "react": "18.1.0",
    "react-i18next": "^12.1.4",
    "react-native": "0.70.6",
    "react-native-card-stack-swiper": "^1.2.5",
    "react-native-date-picker": "^4.2.6",
    "react-native-dotenv": "^3.4.6",
    "react-native-flash-message": "^0.3.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-maps": "^1.3.2",
    "react-native-modal-selector": "^2.1.2",
    "react-native-permissions": "^3.6.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-switch-selector": "^2.3.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-dotenv": "^3.4.6",
